<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.credentialvault.credvault">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.RegisterNewUserActivity" />
        <activity android:name=".activities.HomeActivity" />
        <activity android:name=".activities.NewWebsiteActivity" />
        <activity android:name=".activities.BasicAuthenticationActivity" />
        <activity android:name=".activities.CardListParentMainMenuActivity" />
        <activity android:name=".activities.CardListChildMainMenuActivity" />
        <activity android:name=".activities.CardDetailActivity" />
        <activity android:name=".activities.NotesActivity" />
        <activity android:name=".activities.AllViewerActivity"></activity>
    </application>

</manifest>